import random
from time import sleep
while(1):
    t=random.randint(1,100)
    h=random.randint(1,100)
    print("\ntemperature={0} \nhumidity= {1}".format(t,h))
    if(t>33 or h>80):
        print("Alarm Detected")
    elif((t>27 and t<=33) or (h<30 and h>50)):
        print("Normal Temperature")
    else:
        print("cold")

    sleep(3)
